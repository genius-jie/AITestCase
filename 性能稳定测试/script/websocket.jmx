        <hashTree>
          <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="MP3转Opus转换脚本" enabled="true">
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="script">import java.nio.file.Files
import java.nio.file.Paths

def audioPath = vars.get(&quot;AUDIO_FILE_PATH&quot;)
def audioFile = new File(audioPath)

if (!audioFile.exists()) {
    log.error(&quot;音频文件不存在: &quot; + audioPath)
    return
}

log.info(&quot;开始处理音频文件: &quot; + audioPath + &quot;, 文件大小: &quot; + audioFile.length() + &quot; 字节&quot;)

try {
    def tempPcmFile = File.createTempFile(&quot;temp_pcm_&quot;, &quot;.raw&quot;)
    def tempOpusFile = File.createTempFile(&quot;temp_opus_&quot;, &quot;.opus&quot;)
    
    tempPcmFile.deleteOnExit()
    tempOpusFile.deleteOnExit()
    
    def ffmpegPath = &quot;ffmpeg&quot;
    
    def processBuilder1 = new ProcessBuilder(ffmpegPath, &quot;-y&quot;, &quot;-i&quot;, audioPath, &quot;-f&quot;, &quot;s16le&quot;, &quot;-ar&quot;, &quot;16000&quot;, &quot;-ac&quot;, &quot;1&quot;, tempPcmFile.getAbsolutePath())
    processBuilder1.redirectErrorStream(true)
    def process1 = processBuilder1.start()
    
    def output1 = new StringBuilder()
    process1.inputStream.eachLine { line -&gt;
        output1.append(line).append(&quot;\n&quot;)
        log.info(&quot;FFmpeg (MP3转PCM): &quot; + line)
    }
    
    def exitCode1 = process1.waitFor()
    if (exitCode1 != 0) {
        log.error(&quot;FFmpeg MP3转PCM失败，退出码: &quot; + exitCode1)
        log.error(&quot;输出: &quot; + output1.toString())
        return
    }
    
    log.info(&quot;PCM文件大小: &quot; + tempPcmFile.length() + &quot; 字节&quot;)
    
    def processBuilder2 = new ProcessBuilder(ffmpegPath, &quot;-y&quot;, &quot;-f&quot;, &quot;s16le&quot;, &quot;-ar&quot;, &quot;16000&quot;, &quot;-ac&quot;, &quot;1&quot;, &quot;-i&quot;, tempPcmFile.getAbsolutePath(), &quot;-c:a&quot;, &quot;libopus&quot;, &quot;-b:a&quot;, &quot;64k&quot;, &quot;-frame_duration&quot;, &quot;60&quot;, &quot;-application&quot;, &quot;voip&quot;, tempOpusFile.getAbsolutePath())
    processBuilder2.redirectErrorStream(true)
    def process2 = processBuilder2.start()
    
    def output2 = new StringBuilder()
    process2.inputStream.eachLine { line -&gt;
        output2.append(line).append(&quot;\n&quot;)
        log.info(&quot;FFmpeg (PCM转Opus): &quot; + line)
    }
    
    def exitCode2 = process2.waitFor()
    if (exitCode2 != 0) {
        log.error(&quot;FFmpeg PCM转Opus失败，退出码: &quot; + exitCode2)
        log.error(&quot;输出: &quot; + output2.toString())
        return
    }
    
    log.info(&quot;Opus文件大小: &quot; + tempOpusFile.length() + &quot; 字节&quot;)
    
    def opusBytes = Files.readAllBytes(Paths.get(tempOpusFile.getAbsolutePath()))
    
    def opusTempFile = File.createTempFile(&quot;opus_audio_&quot;, &quot;.opus&quot;)
    opusTempFile.deleteOnExit()
    Files.write(Paths.get(opusTempFile.getAbsolutePath()), opusBytes)
    
    vars.put(&quot;OPUS_AUDIO_FILE&quot;, opusTempFile.getAbsolutePath())
    
    log.info(&quot;音频转换成功！最终Opus数据大小: &quot; + opusBytes.length + &quot; 字节&quot;)
    log.info(&quot;Opus数据已保存到临时文件: &quot; + opusTempFile.getAbsolutePath())
    
    tempPcmFile.delete()
    tempOpusFile.delete()
    
} catch (Exception e) {
    log.error(&quot;音频转换过程中发生错误: &quot; + e.getMessage(), e)
}</stringProp>
