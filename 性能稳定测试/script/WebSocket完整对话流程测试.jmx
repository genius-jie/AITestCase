            <hashTree>
              <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="读取下一帧（从全局共享PCM文件）" enabled="true">
                <stringProp name="scriptLanguage">groovy</stringProp>
                <stringProp name="parameters"></stringProp>
                <stringProp name="filename"></stringProp>
                <stringProp name="cacheKey">false</stringProp>
                <stringProp name="script">import java.io.RandomAccessFile
import java.nio.ByteBuffer
import java.nio.ByteOrder
import io.github.jaredmdobson.concentus.*

String path = vars.get(&quot;PCM_FILE_PATH&quot;)
int frameSize = 1920

if (path == null || path.isEmpty()) {
    log.error(&quot;PCM文件路径未设置&quot;)
    SampleResult.setSuccessful(false)
    SampleResult.setResponseMessage(&quot;PCM文件路径未设置&quot;)
    return
}

// 使用props读取线程级offset，确保在循环中保持状态
String threadOffsetKey = &quot;THREAD_OFFSET_&quot; + ctx.getThreadNum()
String currentOffsetStr = props.get(threadOffsetKey)

// 如果props中没有，尝试从vars读取（兼容性处理）
if (currentOffsetStr == null || currentOffsetStr.isEmpty()) {
    currentOffsetStr = vars.get(&quot;CURRENT_OFFSET&quot;)
    if (currentOffsetStr == null || currentOffsetStr.isEmpty()) {
        currentOffsetStr = &quot;0&quot;
    }
    props.put(threadOffsetKey, currentOffsetStr)
}

long offset = Long.parseLong(currentOffsetStr)

// Get or Create Encoder (Singleton per thread)
OpusEncoder encoder = vars.getObject(&quot;OPUS_ENCODER&quot;)
if (encoder == null) {
    // 16kHz, 1 channel, VOIP mode
    encoder = new OpusEncoder(16000, 1, OpusApplication.OPUS_APPLICATION_VOIP)
    encoder.setBitrate(32000)
    // OPUS_SIGNAL_VOICE is optimized for speech
    encoder.setSignalType(OpusSignal.OPUS_SIGNAL_VOICE)
    vars.putObject(&quot;OPUS_ENCODER&quot;, encoder)
}

RandomAccessFile raf = new RandomAccessFile(path, &quot;r&quot;)
if (offset &lt; raf.length()) {
    raf.seek(offset)
    int bytesToRead = (int) Math.min(frameSize, raf.length() - offset)
    byte[] pcmBuffer = new byte[bytesToRead]
    raf.readFully(pcmBuffer)

    // 1. PCM Bytes -&gt; Shorts (Little Endian)
    // 1920 bytes -&gt; 960 shorts
    short[] shortBuffer = new short[bytesToRead / 2]
    ByteBuffer.wrap(pcmBuffer).order(ByteOrder.LITTLE_ENDIAN).asShortBuffer().get(shortBuffer)
    
    // 2. Pad if necessary (Concentus usually expects standard frame sizes like 10, 20, 40, 60ms)
    // 60ms @ 16kHz = 960 samples
    if (shortBuffer.length &lt; 960) {
        short[] padded = new short[960]
        System.arraycopy(shortBuffer, 0, padded, 0, shortBuffer.length)
        shortBuffer = padded
    }

    // 3. Encode to Opus
    byte[] opusBuffer = new byte[1275] // Max payload size
    int len = encoder.encode(shortBuffer, 0, 960, opusBuffer, 0, opusBuffer.length)
    
    byte[] finalOpus = new byte[len]
    System.arraycopy(opusBuffer, 0, finalOpus, 0, len)

    // 4. Hex Adapter for JMeter Plugin (Send Opus as Hex)
    // ADAPTER: This plugin requires Hex string for binary payload
    StringBuilder hex = new StringBuilder()
    for (byte b : finalOpus) {
        hex.append(String.format(&quot;%02x&quot;, b))
    }
    vars.put(&quot;FRAME_HEX&quot;, hex.toString())

    // 5. Update offset for next iteration (使用props确保状态保持)
    long newOffset = offset + bytesToRead
    props.put(threadOffsetKey, String.valueOf(newOffset))
    vars.put(&quot;CURRENT_OFFSET&quot;, String.valueOf(newOffset))
    
    log.info(&quot;线程 &quot; + ctx.getThreadNum() + &quot; 发送帧: offset=&quot; + offset + &quot; -&gt; &quot; + newOffset + &quot;, bytes=&quot; + bytesToRead)
} else {
    // 已到达文件末尾，发送静音帧
    short[] silenceBuffer = new short[960]
    java.util.Arrays.fill(silenceBuffer, (short) 0)
    
    byte[] opusBuffer = new byte[1275]
    int len = encoder.encode(silenceBuffer, 0, 960, opusBuffer, 0, opusBuffer.length)
    
    byte[] finalOpus = new byte[len]
    System.arraycopy(opusBuffer, 0, finalOpus, 0, len)

    StringBuilder hex = new StringBuilder()
    for (byte b : finalOpus) {
        hex.append(String.format(&quot;%02x&quot;, b))
    }
    vars.put(&quot;FRAME_HEX&quot;, hex.toString())
    
    // 继续更新offset，避免重复处理
    long newOffset = offset + frameSize
    props.put(threadOffsetKey, String.valueOf(newOffset))
    vars.put(&quot;CURRENT_OFFSET&quot;, String.valueOf(newOffset))
    
    log.warn(&quot;线程 &quot; + ctx.getThreadNum() + &quot; 已到达文件末尾，发送静音帧, offset=&quot; + offset + &quot; -&gt; &quot; + newOffset)
}
raf.close()</stringProp>
              </JSR223PreProcessor>
