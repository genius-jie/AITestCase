    def json = new JsonSlurper().parseText(response)
    if (json.type == &quot;tts&quot;) {
      // 处理sentence_start和sentence_end状态，提取并打印text内容
      if (json.state in [ &quot;sentence_end&quot;]) {
        if (json.text) {
          log.info(&quot;收到TTS ${json.state}消息，文本内容：${json.text}&quot;)
        }
      }
      // 原有的stop状态处理逻辑
      if (json.state == &quot;stop&quot;) {
        vars.put(&quot;TTS_STOP_RECEIVED&quot;, &quot;true&quot;)
        log.info(&quot;收到TTS stop消息，结束TTS接收循环&quot;)
      }
    }
  } catch (Exception e) {
    // 不是JSON消息，可能是二进制音频，继续循环
  }
