          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="script">// 获取命令行传入的环境参数，默认为 test
String env = vars.get(&quot;env&quot;) ?: &quot;dev&quot;

// 根据环境动态设置服务器地址和端口
String baseUrl
String port

switch(env) {
    case &quot;dev&quot;:
        baseUrl = vars.get(&quot;dev_base_url&quot;)
        port = vars.get(&quot;dev_port&quot;)
        break
    case &quot;test&quot;:
        baseUrl = vars.get(&quot;test_base_url&quot;)
        port = vars.get(&quot;test_port&quot;)
        break
    case &quot;prod&quot;:
        baseUrl = vars.get(&quot;prod_base_url&quot;)
        port = vars.get(&quot;prod_port&quot;)
        break
    default:
        baseUrl = vars.get(&quot;test_base_url&quot;)
        port = vars.get(&quot;test_port&quot;)
        break
}

// 设置变量供后续使用
vars.put(&quot;base_url&quot;, baseUrl)
vars.put(&quot;port&quot;, port)

log.info(&quot;环境变量初始化完成: env=&quot; + env + &quot;, base_url=&quot; + baseUrl + &quot;, port=&quot; + port)</stringProp>
