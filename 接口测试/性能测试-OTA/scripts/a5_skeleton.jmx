    }
} else if (json.code != 0) {
    if (isCreateSuccess) {
        AssertionResult.setFailure(true)
        AssertionResult.setFailureMessage(&quot;业务状态码错误，期望0，实际&quot; + json.code + &quot;，消息：&quot; + json.message)
    } else {
        log.info(&quot;创建升级包失败场景，业务状态码非0，符合预期&quot;)
    }
} else if (!json.data || !json.data.id) {
    if (isCreateSuccess) {
        AssertionResult.setFailure(true)
