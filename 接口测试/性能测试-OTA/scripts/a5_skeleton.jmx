            <stringProp name="script">import groovy.json.JsonSlurper

def response = prev.getResponseDataAsString()
def json = new JsonSlurper().parseText(response)

// 根据场景类型判断预期结果
def isLoginSuccess = !&quot;login_fail&quot;.equals(vars.get(&quot;scenario_type&quot;))

if (prev.getResponseCode() != &quot;200&quot;) {
    if (isLoginSuccess) {
        AssertionResult.setFailure(true)
        AssertionResult.setFailureMessage(&quot;HTTP状态码错误，期望200，实际&quot; + prev.getResponseCode())
    } else {
        // 登录失败场景，HTTP状态码不是200是可以接受的
        log.info(&quot;登录失败场景，HTTP状态码非200，符合预期&quot;)
    }
} else if (json.code != 0) {
    if (isLoginSuccess) {
        AssertionResult.setFailure(true)
        AssertionResult.setFailureMessage(&quot;业务状态码错误，期望0，实际&quot; + json.code + &quot;，消息：&quot; + json.message)
    } else {
        log.info(&quot;登录失败场景，业务状态码非0，符合预期&quot;)
    }
} else if (!json.data || !json.data.access_token) {
    if (isLoginSuccess) {
        AssertionResult.setFailure(true)
        AssertionResult.setFailureMessage(&quot;响应数据不完整，缺少 access_token&quot;)
    } else {
        log.info(&quot;登录失败场景，缺少 access_token，符合预期&quot;)
    }
}</stringProp>
