        <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="动态参数生成器">
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="script">import java.util.Random

// 安全地获取CSV变量
def scenario_type = vars.get(&quot;scenario_type&quot;) ?: &quot;normal&quot;
def expected_result = vars.get(&quot;expected_result&quot;) ?: &quot;success&quot;
def username = vars.get(&quot;username&quot;) ?: &quot;default_user&quot;
def password = vars.get(&quot;password&quot;) ?: &quot;default_pass&quot;
def captcha_id = vars.get(&quot;captcha_id&quot;) ?: &quot;1&quot;
def captcha_code = vars.get(&quot;captcha_code&quot;) ?: &quot;1234&quot;
def upload_file_path = vars.get(&quot;upload_file_path&quot;) 

// 生成动态设备类型（基于时间戳）
def deviceTypes = [&apos;kikigo&apos;]
def random = new Random()
def deviceType = deviceTypes[random.nextInt(deviceTypes.size())]

// 生成动态版本号（使用时间戳确保唯一性）
def timestamp = System.currentTimeMillis()
def version = &quot;2.3.&quot; + String.format(&quot;%04d&quot;, (timestamp % 10000))

// 生成动态备注（基于场景类型）
def remark = scenario_type + &quot;_测试_&quot; + timestamp

// 生成动态SN（随机字符串）
def chars = &quot;ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789&quot;
def sn = &quot;&quot;
for (int i = 0; i &lt; 10; i++) {
    sn += chars.charAt(random.nextInt(chars.length()))
}

// 生成动态文件路径（如果需要测试不存在的文件）
def filePath = upload_file_path
if (&quot;file_upload_fail&quot;.equals(scenario_type)) {
    filePath = &quot;e:\\\\AI测试用例\\\\接口测试\\\\data\\\\nonexistent_&quot; + timestamp + &quot;.pdf&quot;
}

// 设置变量
vars.put(&quot;device_type&quot;, deviceType)
vars.put(&quot;version&quot;, version)
vars.put(&quot;remark&quot;, remark)
vars.put(&quot;sn&quot;, sn)
vars.put(&quot;file_path&quot;, filePath)

// 特定场景的参数设置
if (&quot;version_conflict&quot;.equals(scenario_type)) {
    // 为版本冲突测试使用固定的版本号
    vars.put(&quot;version&quot;, &quot;2.3.4.011&quot;)
}

// 使用vars.get()获取变量值进行日志记录，避免Groovy插值问题
def final_device_type = vars.get(&quot;device_type&quot;)
def final_version = vars.get(&quot;version&quot;)
def final_sn = vars.get(&quot;sn&quot;)
def final_file_path = vars.get(&quot;file_path&quot;)

log.info(&quot;动态参数生成完成: device_type=&quot; + final_device_type + &quot;, version=&quot; + final_version + &quot;, sn=&quot; + final_sn + &quot;, file_path=&quot; + final_file_path + &quot;, scenario_type=&quot; + scenario_type)</stringProp>
